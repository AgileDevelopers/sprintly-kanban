    "es5-shim": "^4.0.6",
    "exorcist": "^0.3.0",
    "flux": "^2.0.1",
    "fuzzy": "^0.1.0",
    "gulp": "^3.8.10",
    "gulp-csso": "^0.2.9",
    "gulp-less": "^2.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-sourcemaps": "^1.2.8",
    "jquery": "^2.1.3",
