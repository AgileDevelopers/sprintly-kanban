{
  "name": "manifold",
  "version": "0.0.1",
  "devDependencies": {
    "backdash": "^1.1.2-2.4.1",
    "lodash": "^2.4.1",
    "jquery": "^2.1.1",
    "envify": "^1.2.1",
    "react": "^0.10.0",
    "reactify": "^0.13.1",
    "browserify": "^4.2.0",
    "tape": "^2.13.3",
    "watchify": "^0.10.2",
    "backbone-route-control": "^0.1.0",
    "browserify-swap": "^0.2.1",
    "supermodel": "git://github.com/wookiehangover/supermodel.git",
    "topcoat": "git://github.com/topcoat/topcoat.git",
    "topcoat-icons": "^0.2.0"
  },
  "scripts": {
    "test": "mocha test",
    "watch": "watchify app -o public/js/manifold.js -v -d",
    "watch-test": "NODE_PATH=./app:$NODE_PATH watchify test/app -o test/app/bundle.js -v -d",
    "build": "browserify app -o public/js/untaken.js -d && NODE_PATH=./app:$NODE_PATH browserify test/app -o test/app/bundle.js -d",
    "build-production": "NODE_ENV=production browserify app | uglifyjs -cm > public/js/manifold.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-swap",
      "reactify"
    ]
  },
  "browserify-swap": {
    "@packages": [
      "supermodel"
    ],
    "all": {
      "underscore": "lodash",
      "backbone": "backdash"
    }
  }
}
