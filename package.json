    "karma-browserify": "~4.0.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "keymirror": "^0.1.1",
    "lodash": "^2.4.1",
    "mocha": "^2.2.1",
    "moment": "^2.8.4",
    "nodemon": "^1.3.6",
    "oblique-strategies": "^1.0.1",
